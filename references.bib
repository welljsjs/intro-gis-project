@article{Zhong2017,
  title = {Trust in {{Interorganizational Relationships}}: {{A Meta-Analytic Integration}}},
  shorttitle = {Trust in {{Interorganizational Relationships}}},
  author = {Zhong, Weiguo and Su, Chenting and Peng, Jisheng and Yang, Zhilin},
  year = {2017},
  month = apr,
  journal = {Journal of Management},
  volume = {43},
  number = {4},
  pages = {1050--1075},
  issn = {0149-2063, 1557-1211},
  doi = {10.1177/0149206314546373},
  urldate = {2021-11-03},
  abstract = {Prior research on interorganizational trust (IOT) has drawn on multiple theories across disciplines, resulting in mixed findings. This meta-analysis combines the three major theories of IOT, namely, transaction cost economics, social embeddedness theory, and resource dependence theory, to retheorize about these IOT relationships. Specifically, we consolidate 168 tests of IOT across theories and corroborate the additive predictive validity of each of the three theories and their combined explanations on IOT development. In particular, by combining IOT theories, we find an inverted U-shaped relationship between relationship duration and IOT; we also find an intertemporal link among the three IOT theories, and relationship duration as the spanning factor functions to moderate the IOT relationships across theories. These findings serve to reconcile prior conflicting findings and shed new light on IOT development. We conclude our meta-analysis by providing directions for future research.},
  langid = {english},
  file = {/Users/juliusschmidt/Zotero/storage/N5GBGENX/Zhong et al. - 2017 - Trust in Interorganizational Relationships A Meta.pdf}
}
