@article{Villena2019,
  title = {Revisiting {{Interorganizational Trust}}: {{Is More Always Better}} or {{Could More Be Worse}}?},
  shorttitle = {Revisiting {{Interorganizational Trust}}},
  author = {Villena, Ver{\'o}nica H. and Choi, Thomas Y. and Revilla, Elena},
  year = {2019},
  month = feb,
  journal = {Journal of Management},
  volume = {45},
  number = {2},
  pages = {752--785},
  issn = {0149-2063, 1557-1211},
  doi = {10.1177/0149206316680031},
  urldate = {2021-11-03},
  abstract = {This study conducts an investigation of interorganizational trust and its positive and negative effects. We consider how positive and negative effects operate differently under two types of uncertainties\textemdash buyer dependence and market instability. Trust is studied in the buyer\textendash supplier relationship (BSR) context from the buyer's perspective. The analysis is conducted based on survey data and secondary archival data from a sample of 133 BSRs. Results show that trust follows an inverted- U shape with performance. There is a point at which the negative effects of trust offset its benefits, and beyond that point, performance declines. The results also suggest that the positive and negative effects of trust become more pronounced when buyers are highly dependent on suppliers or when environmental uncertainty surrounding buyers is low. Trust's negative effects are more severe for those buyers that are highly dependent and operate in stable markets.},
  langid = {english},
  file = {/Users/juliusschmidt/Zotero/storage/UFKQP3TF/Villena et al. - 2019 - Revisiting Interorganizational Trust Is More Alwa.pdf}
}

